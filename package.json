  "description": "setup nodejs and express",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "ts-node-dev --inspect -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
